{
  "name": "onedocs",
  "version": "0.1.20",
  "type": "module",
  "description": "Zero-config documentation for Next.js + Fumadocs. Install one dependency, write markdown, ship docs.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inline0/onedocs.git"
  },
  "bugs": {
    "url": "https://github.com/inline0/onedocs/issues"
  },
  "homepage": "https://github.com/inline0/onedocs#readme",
  "keywords": [
    "documentation",
    "docs",
    "nextjs",
    "next",
    "fumadocs",
    "mdx",
    "markdown",
    "react"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./source": {
      "types": "./dist/source/index.d.ts",
      "import": "./dist/source/index.js"
    },
    "./llms": {
      "types": "./dist/llms/index.d.ts",
      "import": "./dist/llms/index.js"
    },
    "./seo": {
      "types": "./dist/seo/index.d.ts",
      "import": "./dist/seo/index.js"
    },
    "./og": {
      "types": "./dist/og/index.d.ts",
      "import": "./dist/og/index.js"
    },
    "./metadata": {
      "types": "./dist/metadata/index.d.ts",
      "import": "./dist/metadata/index.js"
    },
    "./css/preset.css": "./src/css/preset.css"
  },
  "files": [
    "dist",
    "src/css",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "bun run clean && tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "prepack": "bun run build"
  },
  "peerDependencies": {
    "fumadocs-core": "^16.4.7",
    "fumadocs-mdx": "^14.2.6",
    "fumadocs-ui": "^16.4.7",
    "lucide-react": "^0.562.0",
    "next": "^15.0.0 || ^16.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@onedocs/tsconfig": "workspace:*",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "fumadocs-core": "^16.4.7",
    "fumadocs-mdx": "^14.2.6",
    "fumadocs-ui": "^16.4.7",
    "lucide-react": "^0.562.0",
    "next": "^16.1.4",
    "typescript": "^5.9.3"
  }
}
